   "source": [
    "# Function to triple a number\n",
    "def triple(n):\n",
    "    return n * 3\n",
    "\n",
    "# List of integers\n",
    "numbers = [1, 2, 3, 4, 5]\n",
    "\n",
    "# Using map to apply the triple function to each element in the list\n",
    "tripled_numbers = list(map(triple, numbers))\n",
    "\n",
    "# Print the result\n",
    "print(tripled_numbers)"
   ]
  }
 ],
